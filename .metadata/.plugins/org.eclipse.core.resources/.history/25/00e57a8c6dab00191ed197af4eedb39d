package com.siwoku.eventcalendar;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

import com.siwoku.eventcalendar.repository.UserRepository;

@SpringBootApplication
public class EventCalendarApplication {
	
	@Autowired
	private static UserRepository userRepository;

	public static void main(String[] args) {
		SpringApplication.run(EventCalendarApplication.class, args);
		
		System.out.println("puto -> " + userRepository.findById(1).get().getName());
	}

}
